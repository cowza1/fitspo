<%=simple_form_for @bio do |f|%>
  <%= f.input  :content, label: "Tell us about yourself"  %>
  <%= f.input  :goal, label: "What are your fitness goals?"  %>
  <%= f.input :body_type, label: "What is your body type?"  %>
  <%= f.input  :hobbies, label: "What are your hobbies?"  %>
  <%= f.submit "Submit", class: "btn btn-success" %>
<%end%>
